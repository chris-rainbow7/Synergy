turn{\r\n            city: {},\r\n\r\n            facilitype: '',\r\n            sport: ''\r\n        }    \r\n    },\r\n    created() {\r\n        APIService.getCities()        \r\n        .then(response => {\r\n        this.city = response.data\r\n      })\r\n      .catch(error => {\r\n        console.log(error)\r\n      })\r\n    },\r\n\r\n    onSubmit() {\r\n        if (this.city === '' && this.facilitype === '' && this.sport=== ''){\r\n            alert(\"You haven't specified your search, so we will be showing everything!\")\r\n        }\r\n\r\n        let mapSearch = {\r\n        facilitype: this.facilitype,\r\n        sport: this.sport\r\n        }\r\n    \r\n        this.$emit('MapSearch-submitted', mapSearch)\r\n\r\n        this.facilitype= ''\r\n        this.sport= ''\r\n        \r\n    }\r\n\r\n}\r\n\r\n</script>"]}]}