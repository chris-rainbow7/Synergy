tion>\r\n                </select>\r\n  \r\n            <input class=\"button\" type=\"submit\" value=\"Submit\">\r\n    </div>\r\n</template>\r\n<script>\r\n\r\nimport APIService from '@/services/APIService.js';\r\n\r\nexport default {\r\n name: \"MapSearch\",\r\n    data() {\r\n        return{\r\n            city: {},\r\n\r\n            facilitype: '',\r\n            sport: ''\r\n        }    \r\n    },\r\n    created() {\r\n        APIService.getCities()        \r\n        .then(response => {\r\n        this.city = response.data\r\n      })\r\n      .catch(error => {\r\n        console.log(error)\r\n      })\r\n    },\r\n\r\n    onSubmit() {\r\n        if (this.city === '' && this.facilitype === '' && this.sport=== ''){\r\n            alert(\"You haven't specified your search, so we will be showing everything!\")\r\n        }\r\n\r\n        let mapSearch = {\r\n        facilitype: this.facilitype,\r\n        sport: this.sport\r\n        }\r\n    \r\n        this.$emit('MapSearch-submitted', mapSearch)\r\n\r\n        this.facilitype= ''\r\n        this.sport= ''\r\n        \r\n    }\r\n\r\n}\r\n\r\n</script>"]}]}