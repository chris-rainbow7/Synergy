r\n\t\tdt_to:''\r\n\t}\r\n  },\r\n  methods:{\r\n\t\t  \r\n\tbuttonpressed(){\r\n\t\t\r\n\t},\r\n\tgetNews(){\r\n\t\tget_News();\r\n\t},\r\n\t\r\n\tasync getNews2(){\r\n      var url='https://newsapi.org/v2/everything?q='+this.q_name+'&apiKey='+this.feedkey+'&from='+this.dt_from+'&to='+this.dt_to;\t\r\n\t\tawait axios\r\n            .get(url\r\n\t\t\t)\r\n            .then(response => (this.artcls = response.data.articles) \r\n        );\r\n\t}\r\n  },\r\n  \r\n}\r\n</script>\r\n\r\n\r\n<style scoped>\r\n\t\r\n  h2 {\r\n    font-size: 40px;\r\n    color: mediumseagreen;\r\n  }\r\n</style>"]}]}